<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:mapbox="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <com.mapbox.mapboxsdk.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="395dp"
        android:layout_height="587dp"
        android:layout_marginStart="5dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        mapbox:layout_constraintBottom_toBottomOf="parent"
        mapbox:layout_constraintEnd_toEndOf="parent"
        mapbox:layout_constraintHorizontal_bias="0.0"
        mapbox:layout_constraintStart_toStartOf="parent"
        mapbox:layout_constraintTop_toTopOf="parent"
        mapbox:mapbox_cameraTargetLat="37.7749"
        mapbox:mapbox_cameraTargetLng="-122.4194"
        mapbox:mapbox_cameraZoom="12" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="600dp"
        android:text="VA"
        mapbox:layout_constraintEnd_toEndOf="parent"
        mapbox:layout_constraintEnd_toStartOf="@+id/mapView"
        mapbox:layout_constraintHorizontal_bias="0.5"
        mapbox:layout_constraintStart_toEndOf="@+id/textView17"
        mapbox:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="600dp"
        android:layout_marginEnd="8dp"
        android:text="Richmond"
        mapbox:layout_constraintEnd_toStartOf="@+id/textView16"
        mapbox:layout_constraintHorizontal_bias="0.5"
        mapbox:layout_constraintStart_toEndOf="@+id/textView18"
        mapbox:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="600dp"
        android:layout_marginEnd="8dp"
        android:text="Example Road"
        mapbox:layout_constraintEnd_toStartOf="@+id/textView17"
        mapbox:layout_constraintHorizontal_bias="0.5"
        mapbox:layout_constraintStart_toEndOf="@+id/textView19"
        mapbox:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView19"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="600dp"
        android:layout_marginEnd="8dp"
        android:text="555"
        mapbox:layout_constraintEnd_toStartOf="@+id/textView18"
        mapbox:layout_constraintHorizontal_bias="0.5"
        mapbox:layout_constraintStart_toEndOf="@+id/mapView"
        mapbox:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="573dp"
        android:text="VA"
        mapbox:layout_constraintEnd_toStartOf="@+id/mapView"
        mapbox:layout_constraintHorizontal_bias="0.5"
        mapbox:layout_constraintStart_toEndOf="@+id/textView6"
        mapbox:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="573dp"
        android:layout_marginEnd="8dp"
        android:text="Richmond"
        mapbox:layout_constraintEnd_toStartOf="@+id/textView7"
        mapbox:layout_constraintHorizontal_bias="0.5"
        mapbox:layout_constraintStart_toEndOf="@+id/textView5"
        mapbox:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="573dp"
        android:layout_marginEnd="8dp"
        android:text="Example Road"
        mapbox:layout_constraintEnd_toStartOf="@+id/textView6"
        mapbox:layout_constraintHorizontal_bias="0.5"
        mapbox:layout_constraintStart_toEndOf="@+id/textView"
        mapbox:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="30dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="573dp"
        android:layout_marginEnd="8dp"
        android:text="555"
        mapbox:layout_constraintEnd_toStartOf="@+id/textView5"
        mapbox:layout_constraintHorizontal_bias="0.5"
        mapbox:layout_constraintStart_toEndOf="@+id/mapView"
        mapbox:layout_constraintTop_toTopOf="parent" />

    <!-- MAP -->

    <Button
        android:id="@+id/button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        android:text="Button"
        mapbox:layout_constraintBottom_toBottomOf="parent"
        mapbox:layout_constraintEnd_toEndOf="parent"
        mapbox:layout_constraintStart_toStartOf="parent" />

    <AutoCompleteTextView
        android:id="@+id/autoCompleteTextView"
        style="@android:style/Widget.Material.AutoCompleteTextView"
        android:layout_width="271dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="108dp"
        android:layout_marginTop="472dp"
        android:layout_marginEnd="200dp"
        android:accessibilityHeading="false"
        android:autoLink="map"
        android:backgroundTint="@color/colorPrimaryDark"
        android:backgroundTintMode="screen"
        android:completionHint="@string/mapbox_plugins_autocomplete_search_hint"
        android:completionThreshold="@android:integer/config_shortAnimTime"
        android:dropDownWidth="wrap_content"
        android:dropDownHeight="wrap_content"
        android:elevation="12dp"
        android:focusable="auto"
        android:hint="@string/mapbox_plugins_autocomplete_search_hint"
        android:imeOptions="actionSearch"
        android:inputType="textAutoComplete"
        android:popupBackground="@color/colorPrimaryDark"
        android:text="@string/mapbox_plugins_autocomplete_search_hint"
        android:textAppearance="@android:style/TextAppearance.Widget.DropDownHint"
        mapbox:layout_constraintEnd_toEndOf="parent"
        mapbox:layout_constraintHorizontal_bias="0.226"
        mapbox:layout_constraintStart_toStartOf="parent"
        mapbox:layout_constraintTop_toTopOf="parent"
        tools:text="@android:string/autofill" />

